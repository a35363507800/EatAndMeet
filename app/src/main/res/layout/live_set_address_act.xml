<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoLinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:background="@color/C0333"
	android:orientation="vertical">
	<com.echoesnet.eatandmeet.views.widgets.TopBarSwich.TopBarSwitch
		android:id="@+id/top_bar"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		app:topbarType="TEXT"/>

	<com.zhy.autolayout.AutoRelativeLayout
		android:id="@+id/ll_choose_area"
		android:layout_width="match_parent"
		android:layout_height="88px"
		android:layout_marginTop="16px"
		android:background="@color/white"
	>
		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_centerVertical="true"
			android:layout_marginLeft="24px"
			android:text="所在地区"
			android:textColor="@color/C0321"
			android:textSize="@dimen/f3"/>
		<TextView
			android:id="@+id/tv_area"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_centerVertical="true"
			android:layout_marginRight="30px"
			android:layout_toLeftOf="@+id/itv_go"
			android:text="请选择"
			android:textColor="@color/C0321"
			android:textSize="@dimen/f3"/>
		<com.joanzapata.iconify.widget.IconTextView
			android:id="@+id/itv_go"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_alignParentRight="true"
			android:layout_centerVertical="true"
			android:layout_marginRight="24px"
			android:text="{eam-e909}"
		/>
	</com.zhy.autolayout.AutoRelativeLayout>
	<com.zhy.autolayout.AutoRelativeLayout
		android:id="@+id/ll_choose_road"
		android:layout_width="match_parent"
		android:layout_height="88px"
		android:layout_marginTop="16px"
		android:background="@color/white"
	>
		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_centerVertical="true"
			android:layout_marginLeft="24px"
			android:text="街道"
			android:textColor="@color/C0321"
			android:textSize="@dimen/f3"/>
		<TextView
			android:id="@+id/tv_road"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_centerVertical="true"
			android:layout_marginRight="30px"
			android:layout_toLeftOf="@+id/itv_go1"
			android:text="请选择"
			android:textColor="@color/C0321"
			android:textSize="@dimen/f3"/>
		<com.joanzapata.iconify.widget.IconTextView
			android:id="@+id/itv_go1"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_alignParentRight="true"
			android:layout_centerVertical="true"
			android:layout_marginRight="24px"
			android:text="{eam-e909}"
		/>
	</com.zhy.autolayout.AutoRelativeLayout>

</com.zhy.autolayout.AutoLinearLayout>