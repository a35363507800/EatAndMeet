<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
	android:id="@+id/lLayout_bg"
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="340dp"
	android:layout_height="225dp"
	android:layout_marginLeft="18dp"
	android:layout_marginRight="18dp"
	android:background="@drawable/alert_round_bg_normal"
	android:orientation="vertical"
	android:paddingBottom="25dp"
	android:paddingLeft="15dp"
	android:paddingRight="15dp"
	android:paddingTop="13dp">
	<RelativeLayout
		android:id="@+id/all_content_container"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:gravity="center_horizontal"
		android:orientation="horizontal">
	</RelativeLayout>
	<com.zhy.autolayout.AutoLinearLayout
		android:layout_width="match_parent"
		android:layout_height="44dp"
		android:orientation="horizontal"
	>
		<Button
			android:id="@+id/btn_neg"
			android:layout_width="130dp"
			android:layout_height="match_parent"
			android:layout_marginLeft="10dp"
			android:background="@drawable/egg_count_bg0313"
			android:gravity="center"
			android:text="不"
			android:textColor="@color/C0313"
			android:textSize="@dimen/f2"/>
		<!--<View
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:layout_weight="1"/>-->

		<Button
			android:id="@+id/btn_pos"
			android:layout_width="130dp"
			android:layout_height="match_parent"
			android:layout_marginLeft="30dp"
			android:background="@drawable/round_btn_c0313"
			android:gravity="center"
			android:text="移 除"
			android:textColor="@color/C0324"
			android:textSize="@dimen/f2"/>
	</com.zhy.autolayout.AutoLinearLayout>
</LinearLayout>