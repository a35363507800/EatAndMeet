<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoLinearLayout
	android:id="@+id/all_root_view"
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical"
	android:background="@color/main_background_color"
	tools:context="com.echoesnet.eatandmeet.activities.PromotionActionAct"
	xmlns:app="http://schemas.android.com/apk/res-auto">
	<com.echoesnet.eatandmeet.views.widgets.TopBarSwich.TopBarSwitch
		android:id="@+id/top_bar_switch"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		app:topbarType="TEXT"/>
	<com.echoesnet.eatandmeet.views.widgets.ProgressBridgeWebView.ProgressBridgeWebView
		android:id="@+id/webView_promotion"
		android:layout_width="match_parent"
		android:layout_height="match_parent"/>
</com.zhy.autolayout.AutoLinearLayout>
