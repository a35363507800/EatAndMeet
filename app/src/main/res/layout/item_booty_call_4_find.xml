<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
									   xmlns:app="http://schemas.android.com/apk/res-auto"
									   android:layout_width="100dp"
									   android:layout_height="150dp"
									   android:paddingLeft="6dp">
	<com.makeramen.roundedimageview.RoundedImageView
		android:id="@+id/img_uer_head"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		app:riv_border_color="@color/C0332"
		app:riv_border_width="1px"/>
	<com.zhy.autolayout.AutoRelativeLayout
		android:id="@+id/rl_information"
		android:layout_width="match_parent"
		android:layout_height="wrap_content">
		<View
			android:layout_width="match_parent"
			android:layout_height="90dp"
			android:layout_alignParentBottom="true"
			android:background="@drawable/booty_call_shade_bg"/>
		<com.zhy.autolayout.AutoRelativeLayout
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:layout_alignParentBottom="true"
			android:gravity="center_vertical"
			android:orientation="horizontal"
			android:paddingLeft="6dp"
			android:paddingBottom="6dp">

			<com.echoesnet.eatandmeet.views.widgets.LevelView
				android:id="@+id/level_view"
				android:layout_width="wrap_content"
				android:layout_height="13dp"
				android:layout_marginRight="10px"/>

			<ImageView
				android:layout_toRightOf="@+id/level_view"
				android:id="@+id/img_sex"
				android:layout_width="12dp"
				android:layout_height="13dp"
				android:layout_marginEnd="2dp"
				android:layout_marginRight="2dp"
				/>
			<TextView
				android:id="@+id/tv_user_name"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_below="@+id/img_sex"
				android:ellipsize="end"
				android:lines="1"
				android:text="dsfsfsfs"
				android:textColor="@color/C0324"
				android:layout_marginTop="10px"
				android:textSize="@dimen/f5"/>
		</com.zhy.autolayout.AutoRelativeLayout>
	</com.zhy.autolayout.AutoRelativeLayout>

</com.zhy.autolayout.AutoRelativeLayout>