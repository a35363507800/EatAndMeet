<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoRelativeLayout
	android:id="@+id/itemClick"
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	android:layout_width="match_parent"
	android:layout_height="68dp"
	android:background="@color/white"
	android:paddingLeft="@dimen/activity_horizontal_margin">
	<View
		android:layout_width="match_parent"
		android:layout_height="0.8dp"
		android:layout_alignParentTop="true"
		android:background="@color/FC7"/>
	<com.echoesnet.eatandmeet.views.widgets.LevelHeaderView
		android:id="@+id/img_item_search_head"
		android:layout_width="50dp"
		android:layout_height="50dp"
		android:layout_centerVertical="true"/>
	<LinearLayout
		android:id="@+id/ll_container"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginLeft="20px"
		android:layout_alignTop="@+id/img_item_search_head"
		android:layout_marginTop="10dp"
		android:layout_toRightOf="@+id/img_item_search_head"
		android:orientation="horizontal">
		<com.echoesnet.eatandmeet.views.widgets.LevelView
			android:id="@+id/level_view"
			android:layout_width="wrap_content"
			android:layout_height="15dp"
			android:layout_gravity="center_vertical"
		/>
		<com.joanzapata.iconify.widget.IconTextView
			android:id="@+id/iv_item_search_sex"
			android:layout_width="31dp"
			android:layout_height="15dp"
			android:gravity="center"
			android:layout_gravity="center_vertical"
			android:layout_marginLeft="10px"
			android:textColor="@color/white"
			android:textSize="@dimen/f5"/>
		<TextView
			android:id="@+id/tv_item_search_name"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_gravity="center_vertical"
			android:layout_marginLeft="10px"
			android:text="小苍"
			android:textColor="@color/FC1"
			android:textSize="@dimen/f3"/>
	</LinearLayout>
	<TextView
		android:id="@+id/tv_item_search_id"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginLeft="20px"
		android:layout_below="@+id/ll_container"
		android:layout_centerVertical="true"
		android:layout_marginTop="5dp"
		android:layout_toRightOf="@+id/img_item_search_head"
		android:text="ID:666666"
		android:textColor="@color/FC3"
		android:textSize="@dimen/f4"/>
	<com.joanzapata.iconify.widget.IconTextView
		android:id="@+id/iv_item_search_living"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_alignParentRight="true"
		android:layout_centerVertical="true"
		android:layout_marginRight="@dimen/activity_horizontal_margin"
		android:text="{eam-e980 @color/MC1}  直播中"
		android:textColor="@color/MC1"
		android:visibility="visible"/>

</com.zhy.autolayout.AutoRelativeLayout>