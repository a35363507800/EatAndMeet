<?xml version="1.0" encoding="utf-8"?>

<com.zhy.autolayout.AutoLinearLayout
	android:id="@+id/contact_us_pop_model"
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="match_parent"
	android:layout_height="wrap_content"
	android:layout_alignParentBottom="true"
	android:background="@color/white"
	android:orientation="vertical">
	<TextView
		android:layout_width="match_parent"
		android:layout_height="114px"
		android:background="@color/FC4"
		android:gravity="center"
		android:text="联系我们"
		android:textColor="@color/FC3"
		android:textSize="44px"/>
	<View android:layout_width="match_parent"
		  android:layout_height="1px"
		  android:background="@color/FC7"/>
	<ListView
		android:id="@+id/lv_contact_lst"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:listSelector="@color/transparent"
		android:divider="@color/c5"
		android:dividerHeight="1px">
	</ListView>
	<Button
		android:id="@+id/my_set_contact_us_cancle"
		style="@style/commonButton4"
		android:layout_width="match_parent"
		android:layout_height="98px"
		android:text="取消"
		android:textColor="@color/white"
		android:textSize="32px"/>
</com.zhy.autolayout.AutoLinearLayout>

