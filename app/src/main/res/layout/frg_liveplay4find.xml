<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/white">

    <com.handmark.pulltorefresh.library.PullToRefreshScrollView
        android:id="@+id/pull_to_refresh_sv_live"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scrollbars="none">

        <com.zhy.autolayout.AutoLinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <com.echoesnet.eatandmeet.views.widgets.ImageIndicatorView.NetworkImageIndicatorView
                android:id="@+id/icv_cycle_view_live"
                android:layout_width="match_parent"
                android:layout_height="180px" />
            <com.zhy.autolayout.AutoLinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <com.joanzapata.iconify.widget.IconTextView
                    android:id="@+id/itv_booty_call"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="12px"
                    android:text="{eam-e9bb}"
                    android:textColor="@color/C0313"
                    android:textSize="@dimen/f2" />

                <TextView
                    android:id="@+id/tv_booty_call_title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/C0321"
                    android:text="@string/date_anchor"
                    android:textSize="@dimen/f4" />
            </com.zhy.autolayout.AutoLinearLayout>

            <android.support.v7.widget.RecyclerView
                android:id="@+id/recyclerview_booty_call"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingBottom="30px"/>
            <View
                android:layout_width="match_parent"
                android:layout_height="10dp"
                android:background="@color/C0333"/>
            <com.zhy.autolayout.AutoLinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <com.joanzapata.iconify.widget.IconTextView
                    android:id="@+id/itv_art_anchor"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="12px"
                    android:text="{eam-e9bc}"
                    android:textColor="@color/C0412"
                    android:textSize="@dimen/f2" />

                <TextView
                    android:id="@+id/tv_art_anchor_title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/C0321"
                    android:text="@string/art_anchor"
                    android:textSize="@dimen/f4" />
                <View
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"/>
            <TextView
                android:id="@+id/tv_art_anchor_more"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/f4"
                android:textColor="@color/C0322"
                android:layout_marginRight="12dp"
                android:text="@string/check_more"/>
            </com.zhy.autolayout.AutoLinearLayout>

            <android.support.v7.widget.RecyclerView
                android:id="@+id/recyclerview_art_anchor"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingBottom="30px"/>
            <View
                android:layout_width="match_parent"
                android:layout_height="10dp"
                android:background="@color/C0333"/>
            <com.zhy.autolayout.AutoLinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <com.joanzapata.iconify.widget.IconTextView
                    android:id="@+id/itv_nearby_anchor"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="12px"
                    android:text="{eam-e9bd}"
                    android:textColor="@color/C0315"
                    android:textSize="@dimen/f2" />

                <TextView
                    android:id="@+id/tv_nearby_anchor_title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/C0321"
                    android:text="@string/nearby_anchor"
                    android:textSize="@dimen/f4" />
                <View
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"/>
            <TextView
                android:id="@+id/tv_nearby_anchor_more"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="12dp"
                android:textSize="@dimen/f4"
                android:textColor="@color/C0322"
                android:text="@string/check_more"/>
            </com.zhy.autolayout.AutoLinearLayout>

            <android.support.v7.widget.RecyclerView
                android:id="@+id/recyclerview_nearby_anchor"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingBottom="30px"/>
            <View
                android:layout_width="match_parent"
                android:layout_height="10dp"
                android:background="@color/C0333"/>
        </com.zhy.autolayout.AutoLinearLayout>

    </com.handmark.pulltorefresh.library.PullToRefreshScrollView>
</com.zhy.autolayout.AutoLinearLayout>